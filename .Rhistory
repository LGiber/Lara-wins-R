step <- function(x, length, m)
{
if (length<1) x
minDist <- Inf
h <- 0
for (i in seq(from=1,to=length-1))
{
if (x[i+1]-x[i]<minDist) {
minDist = abs(x[i+1]-x[i])
h <- i
}
}
x[h] <- (x[h+1]-x[h])/2+x[h]
if (h<length-1)
for (i in seq(from=h+1,to=length-1))
x[i] <- x[i+1]
x[length]=NaN
m = c(m,minDist)
x
}
